{
    "comments": {
        "lineComment": "//",
        "blockComment": [
            "/*",
            "*/"
        ]
    },
    "brackets": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "<",
            ">"
        ]
    ],
    "autoClosingPairs": [
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "<",
            "close": ">"
        },
        {
            "open": "\"",
            "close": "\""
        }
    ],
    "surroundingPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "<",
            ">"
        ],
        [
            "\"",
            "\""
        ]
    ],
    "wordPattern": "[A-Za-z_][A-Za-z$_0-9@]*",
    "indentationRules": {
        "increaseIndentPattern": "^.*\\{[^}]*$",
        "decreaseIndentPattern": "^\\s*\\}"
    }
}